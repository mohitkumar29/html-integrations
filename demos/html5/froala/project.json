{
  "root": "demos/html5/froala",
  "sourceRoot": "demos/html5/froala/src",
  "targets": {
    "start": {
      "executor": "nx:run-script",
      "dependsOn": ["^build"],
      "options": {
          "script": "start"
      }
    },
    "start-remote": {
      "executor": "nx:run-script",
      "options": {
          "script": "start-remote"
      }
    },
    "build": {
      "executor": "@nrwl/web:webpack",
      "dependsOn": ["start"],
      "outputs": ["{options.outputPath}"],
      "options": {
        "main": "demos/html5/froala/src/app.js",
        "outputPath": "dist",
        "tsConfig": "tsconfig.app.json",
        "generateIndexHtml": false,
        "index": "",
        "webpackConfig": "demos/html5/froala/webpack.config.js"
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "dependsOn": ["start"],
      "options": {
        "buildTarget": "html-froala:build",
        "webpackConfig": "demos/html5/froala/webpack.config.js"
      }
    },
    "serve-remote": {
      "executor": "@nrwl/web:dev-server",
      "dependsOn": ["start-remote"],
      "options": {
        "buildTarget": "html-froala:build",
        "webpackConfig": "demos/html5/froala/webpack.config.js"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "eslintConfig": "./.eslintrc.js",
        "lintFilePatterns": ["demos/html5/froala/**/*.{ts,tsx,js,jsx}"]
      },
      "outputs": ["{options.outputFile}"]
    }
  }
}
